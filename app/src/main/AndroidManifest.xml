<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.flaminius2k18.app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.flaminius2k18.app.activity.SplashActivity"
            android:theme="@style/AppTheme.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.flaminius2k18.app.activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name="com.flaminius2k18.app.activity.EventInfoActivity" />
        <activity android:name="com.flaminius2k18.app.activity.FlaminiusActivity" />
        <activity android:name="com.flaminius2k18.app.activity.RegisterActivity" />
        <activity android:name="com.flaminius2k18.app.activity.GalleryActivity" />
        <activity android:name="com.flaminius2k18.app.activity.CollegeActivity" />
        <activity android:name="com.flaminius2k18.app.activity.ContactUsActivity" />
    </application>

</manifest>